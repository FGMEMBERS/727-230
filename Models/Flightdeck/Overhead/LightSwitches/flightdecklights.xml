<PropertyList>

  <path>flightdecklights.ac</path>


	<animation>
		<name>flightdecklights</name>
		<object-name>lightTurn</object-name>
		<object-name>lightTurn.001</object-name>
		<object-name>lightTurn.002</object-name>
		<object-name>lightTurn.003</object-name>
	</animation>
  
	<animation>
		<type>material</type>
		<object-name>lightTurn</object-name>
		<object-name>lightTurn.001</object-name>
		<object-name>lightTurn.002</object-name>
		<object-name>lightTurn.003</object-name>
		<condition>	
			<not><property>/sim/rendering/rembrandt/enabled</property></not>
		</condition>
		<emission>
		  <red>0.4</red>
		  <green>0.4</green>
		  <blue>0.4</blue>
		  <factor-prop>/systems/electrical/outputs/overhead-dim</factor-prop>
		</emission>
	</animation>
  
  <!-- cabin dim --> 
	<animation>
		<type>rotate</type>
		<object-name>lightTurn</object-name>
		<property>/controls/lighting/cabin-dim</property>
		<factor>-225</factor>
		<axis>
		 <x>1</x>
		 <y>0</y>
		 <z>0</z>
		</axis>
		<center>
		<x-m> 0.0</x-m>
		<y-m>-0.2469</y-m>
		<z-m>-0.2</z-m>
		</center>  
	</animation>
     
	<animation>
		<type>pick</type>
		<object-name>lightTurn</object-name>
		<action>
		  <button>0</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/lighting/cabin-dim</property>
		    <step>0.1</step>
		    <min>0</min>
		    <max>1</max>
		    <wrap>0</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>/sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>1</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/lighting/cabin-dim</property>
		    <step>-0.1</step>
		    <min>0</min>
		    <max>1</max>
		    <wrap>0</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>/sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>3</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/lighting/cabin-dim</property>
		    <step>0.1</step>
		    <min>0</min>
		    <max>1</max>
		    <wrap>0</wrap>
		  </binding>
		  <binding>
		    <command>property-toggle</command>
		    <property>/sim/sound/switch3</property>
		  </binding>
		</action>
		<action>
		  <button>4</button>
		  <repeatable>true</repeatable>
		  <binding>
		    <command>property-adjust</command>
		    <property>/controls/lighting/cabin-dim</property>
